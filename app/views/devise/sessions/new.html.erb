
<div class="container d-flex justify-content-center " style="margin-top:100px"></div>
            <h2 class="d-flex justify-content-center" href="/"><%= image_tag 'KakaoTalk_20170818_233541813.png'%></h2>

<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
   <div class=" d-flex justify-content-center  " style="margin-top:70px">
  <h2 class="d-flex justify-content-center" style="background-image:3.png"></h2>

  <div class="form-inputs  ">
   <div class="d-flex justify-content-center mx-auto w-75  ">
    <%= f.input :email   , required: false, autofocus: true %><br></div>
  
       
<div class="w-75 mx-auto  mt-2" >
    <%= f.input :password, required: false %><br> </div>
    <div class="text-center mt-2" >

    <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
  </div>
  </div>
  </div>

  <div class="form-actions d-flex justify-content-center " style="margin-top:30px">
    <%= f.button :submit, "로그인" ,  class:'btn btn-secondary '  %>
       <%= render "devise/shared/links"%>
    </div>
  
<% end %>

  
  <div class="d-flex justify-content-center">
     
   </div></div>